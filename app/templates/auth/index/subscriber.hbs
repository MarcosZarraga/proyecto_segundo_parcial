{{outlet}}

<form {{action 'addSubs' on='submit'}}>
  <div class="form-group">
    <label >Nombre</label>
    {{input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Añadir Nombre" value=name}}

  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Apellido</label>
    {{input type="text" class="form-control" id="exampleInputPassword1" placeholder="Añadir Apellido" value=lastName}}
  </div>
  <button type="submit" class="btn btn-primary" >Añadir Subscriptor</button>
</form>
