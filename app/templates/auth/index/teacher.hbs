{{outlet}}

<form {{action 'addTeacher' on='submit'}}>

    <div class="form-group">
    <label >Nombre</label>
    {{input type="text" class="form-control" placeholder="Añadir Nombre" value=name}}

  </div>
  <div class="form-group">
    <label >Apellido</label>
    {{input type="text" class="form-control"  placeholder="Añadir Apellido" value=last_name}}

  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Telefono</label>
    {{input type="number" class="form-control"  placeholder="Telefono" value=phone}}
  </div>

    <div class="form-group">
    <label >Email</label>
    {{input type="text" class="form-control"  placeholder="Email" value=email}}

  </div>
  <button type="submit" class="btn btn-primary" >Añadir Maestro</button>
</form>


